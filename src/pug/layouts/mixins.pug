mixin card-info(ind,img,title,text1,text2,label)
	.card-info(data-index=''+ind)&attributes(attributes)
		.card-info__icon
			img(src="img/emoji/"+img+".svg" alt='emoji')
		.card-info__text
			h4.title-4 #{title}
			p !{text1}
			p !{text2}

		if label
			.card-label #{label}


mixin card-team(ind,img,title,text2,label,icon)
	.card-team(data-index=''+ind)&attributes(attributes)
		.card-team__img
			img.lazy(data-src="img/team/" + img + ".jpg" alt='team')
		.card-info__text
			h4.title-4 #{title}
			p !{text2}

		if label
			.card-label #{label}
		if icon
			.card-icon
				img(src="img/team/" + icon + ".svg" alt='icon')
// кнопка для слабовидящих
mixin btn-visual
	button.button.button--visual
		span
			+eye-icon
		span Версия для слабовидящих

